journal = {Proceedings. Biological sciences / The Royal Society},
keywords = {Adult,Ankle Joint,Ankle Joint: physiology,Biomechanics,Electromyography,Energy Metabolism,Exercise Test,Humans,Isometric Contraction,Isometric Contraction: physiology,Knee Joint,Knee Joint: physiology,Male,Muscles,Muscles: physiology,Tendons,Tendons: physiology,Walking,Walking: physiology},
month = feb,
number = {1464},
pages = {229--33},
pmid = {11217891},
title = {{In vivo behaviour of human muscle tendon during walking.}},
