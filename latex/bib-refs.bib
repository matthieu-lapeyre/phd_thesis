@article{Masani2002,
abstract = {The purpose of this study was to investigate whether or not the neuromuscular locomotor system is optimized at a unique speed by examining the variability of the ground reaction force (GRF) pattern during walking in relation to different constant speeds. Ten healthy male subjects were required to walk on a treadmill at 3.0, 4.0, 5.0, 6.0, 7.0, and 8.0 km/h. Three components [vertical (F(z)), anteroposterior (F(y)), and mediolateral (F(x)) force] of the GRF were independently measured for approximately 35 steps consecutively for each leg. To quantify the GRF pattern, five indexes (first and second peaks of F(z), first and second peaks of F(y), and F(x) peak) were defined. Coefficients of variation were calculated for these five indexes to evaluate the GRF variability for each walking speed. It became clear for first and second peaks of F(z) and F(x) peak that index variabilities increased in relation to increments in walking speed, whereas there was a speed (5.5-5.8 km/h) at which variability was minimum for first and second peaks of F(y), which were related to forward propulsion of the body. These results suggest that there is "an optimum speed" for the neuromuscular locomotor system but only for the propulsion control mechanism.},
author = {Masani, Kei and Kouzaki, Motoki and Fukunaga, Tetsuo},
doi = {10.1152/japplphysiol.00969.2000},
file = {:Users/matthieulapeyre/Library/Application Support/Mendeley Desktop/Downloaded/Masani, Kouzaki, Fukunaga - 2002 - Variability of ground reaction forces during treadmill walking.pdf:pdf},
issn = {8750-7587},
journal = {Journal of applied physiology (Bethesda, Md. : 1985)},
keywords = {Adult,Exercise Test,Humans,Locomotion,Locomotion: physiology,Male,Reproducibility of Results,Stress, Mechanical,Walking,Walking: physiology},
month = may,
number = {5},
pages = {1885--90},
pmid = {11960938},
title = {{Variability of ground reaction forces during treadmill walking.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/11960938},
volume = {92},
year = {2002}
}
