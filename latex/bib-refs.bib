month = jan,
number = {1},
pages = {45--53; discussion 54--6},
pmid = {1728048},
title = {{Foot trajectory in human gait: a precise and multifactorial motor control task.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/1728048},
volume = {72},
year = {1992}
